<ScrollPane fx:id="mainPane" hbarPolicy="NEVER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mytwitterphase2.FXMLController.SignInController">
   <content>
      <AnchorPane fx:id="anchorPane" prefHeight="397.0" prefWidth="598.0">
         <children>
            <ImageView fx:id="twitterImg" fitHeight="119.0" fitWidth="106.0" layoutX="246.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Photo/Project/icons8-twitter-400.png" />
               </image>
            </ImageView>
            <Text fx:id="usernameText" layoutX="186.0" layoutY="146.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <TextField fx:id="usernameField" editable="false" layoutX="186.0" layoutY="150.0" prefHeight="35.0" prefWidth="227.0" promptText="Username">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <PasswordField fx:id="passwordField" layoutX="186.0" layoutY="196.0" prefHeight="35.0" prefWidth="227.0" promptText="Password">
               <font>
                  <Font size="20.0" />
               </font>
            </PasswordField>
            <Text fx:id="forgetText" fill="#78a1d1" layoutX="239.0" layoutY="272.0" onMouseClicked="#onForgetClicked" strokeType="OUTSIDE" strokeWidth="0.0" text="Forget password?" wrappingWidth="354.69775390625">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text fx:id="enterText" fill="#78a1d1" layoutX="171.0" layoutY="117.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter your password" wrappingWidth="426.58203125">
               <font>
                  <Font size="30.0" />
               </font>
            </Text>
            <Text fx:id="accountText" layoutX="190.0" layoutY="355.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Don't have an acoount?" wrappingWidth="412.0859375">
               <font>
                  <Font size="16.0" />
               </font>
            </Text>
            <Text fx:id="signUpText" fill="#78a1d1" layoutX="352.0" layoutY="354.0" onMouseClicked="#onSignUpClicked" strokeType="OUTSIDE" strokeWidth="0.0" text="Sign up" wrappingWidth="248.5">
               <font>
                  <Font size="16.0" />
               </font>
            </Text>
            <Rectangle fx:id="logInRect" arcHeight="5.0" arcWidth="5.0" fill="#78a1d1" height="40.0" layoutX="203.0" layoutY="276.0" onMouseClicked="#onLogInClicked" stroke="#78a1d1" strokeType="INSIDE" width="193.0" />
            <Circle fx:id="circ1" fill="#78a1d1" layoutX="203.0" layoutY="296.0" onMouseClicked="#onLogInClicked" radius="20.0" stroke="#78a1d1" strokeType="INSIDE" />
            <Circle fx:id="circ2" fill="#78a1d1" layoutX="396.0" layoutY="296.0" onMouseClicked="#onLogInClicked" radius="20.0" stroke="#78a1d1" strokeType="INSIDE" />
            <Text fx:id="logInText" fill="WHITE" layoutX="274.0" layoutY="303.0" onMouseClicked="#onLogInClicked" strokeType="OUTSIDE" strokeWidth="0.0" text="Log in">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <Text fx:id="wrongText" fill="#f21919" layoutX="186.0" layoutY="248.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wrong password! Try again">
               <font>
                  <Font size="12.0" />
               </font>
            </Text>
