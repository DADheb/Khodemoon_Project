<ScrollPane fx:id="mainPane" hbarPolicy="NEVER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demomytwitter.SignInDetailController">
   <content>
      <AnchorPane fx:id="anchorPane" prefHeight="408.0" prefWidth="601.0">
         <children>
            <ImageView fx:id="twitterImg" fitHeight="101.0" fitWidth="102.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@icons8-twitter-400.png" />
               </image>
            </ImageView>
            <Text fx:id="completeText" fill="#78a1d1" layoutX="101.0" layoutY="43.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Compelete your information" wrappingWidth="600">
