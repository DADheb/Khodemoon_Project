                  <Font size="30.0" />
               </font>
            </Text>
            <TextField fx:id="nameField" layoutX="285.0" layoutY="60.0" prefHeight="35.0" prefWidth="227.0" promptText="Name">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <TextField fx:id="lastnameField" layoutX="285.0" layoutY="107.0" prefHeight="35.0" prefWidth="227.0" promptText="Lastname">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <TextField fx:id="emailField" layoutX="285.0" layoutY="203.0" prefHeight="35.0" prefWidth="227.0" promptText="XXX@gmail.com">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <TextField fx:id="bioField" layoutX="285.0" layoutY="251.0" prefHeight="35.0" prefWidth="227.0" promptText="Biography">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <Text fx:id="nameText" layoutX="188.0" layoutY="88.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <Text fx:id="lastnameText" layoutX="182.0" layoutY="134.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Lastname:">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <Text fx:id="birthdayText" layoutX="188.0" layoutY="182.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Birthday:">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <Text fx:id="gmailText" layoutX="199.0" layoutY="230.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Gmail:">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <Text fx:id="bioText" layoutX="181.0" layoutY="278.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Biography:">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <Text fx:id="typeText" layoutX="202.0" layoutY="324.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Type:">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <DatePicker fx:id="birthdayPick" layoutX="285.0" layoutY="155.0" prefHeight="40.0" prefWidth="227.0" promptText="Birthday" />
            <RadioButton fx:id="publicButton" layoutX="290.0" layoutY="296.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" selected="true" text="Public">
               <toggleGroup>
                  <ToggleGroup fx:id="accountType" />
               </toggleGroup>
               <font>
                  <Font size="20.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="privateButton" layoutX="399.0" layoutY="296.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="99.0" text="Private" toggleGroup="$accountType">
