                  <Font size="20.0" />
               </font>
            </RadioButton>
            <Rectangle fx:id="doneRect" arcHeight="5.0" arcWidth="5.0" fill="#78a1d1" height="40.0" layoutX="189.0" layoutY="346.0" onMouseClicked="#onDoneClicked" stroke="#78a1d1" strokeType="INSIDE" width="193.0" />
            <Circle fx:id="doneCirc1" fill="#78a1d1" layoutX="189.0" layoutY="366.0" onMouseClicked="#onDoneClicked" radius="20.0" stroke="#78a1d1" strokeType="INSIDE" />
            <Circle fx:id="doneCirc2" fill="#78a1d1" layoutX="379.0" layoutY="366.0" onMouseClicked="#onDoneClicked" radius="20.0" stroke="#78a1d1" strokeType="INSIDE" />
