        <Label fx:id="usernameL" layoutX="190.0" layoutY="18.0" onMouseClicked="#showUserPro" text="UserName">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <Button fx:id="deleteB" layoutX="500.0" layoutY="28.0" mnemonicParsing="false" onAction="#deleteRequest" text="Delete" textAlignment="CENTER">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Label fx:id="nameL" layoutX="190.0" layoutY="52.0" onMouseClicked="#showUserPro" text="name">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
      <Button fx:id="confirmB" layoutX="392.0" layoutY="28.0" mnemonicParsing="false" onAction="#confirmRequest" text="Confirm" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
    </children>
</Pane>
